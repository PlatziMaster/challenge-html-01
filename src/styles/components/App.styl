@import './styles.styl'
@import url('https://fonts.googleapis.com/css?family=Lato|Oswald:400,500&display=swap')

*
  margin: 0
  padding: 0
  list-style: none
  text-decoration: none


body
  background-color: $bg-color
  min-height: 100vh
  font-family $font-family

input
  font-family $font-family
  font-size  14px
  padding 0 16px

#app
  height 100vh
  display: flex
  flex-direction: column
  justify-content space-between

