@font-face
  font-family: Lato
  font-style normal
  src: url(./fonts/Lato/Lato-Regular.ttf)

body
  background-color: rgb(240,240,240)
  font-family: Lato, Arial, Helvetica, sans-serif

* {
  margin 0
  padding 0
  border 0
  box-sizing border-box
}

.flex-container
  width 100%
  display flex
  align-items center
  flex-wrap wrap

.flex-between
  justify-content space-between
