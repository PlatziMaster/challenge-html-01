*
  margin: 0px;
  padding: 0px;
  list-style: none;
  text-decoration: none;

header nav
  height 70px
  border-bottom 1px solid #EAEAEB
  text-align right
  line-height 70px
  display flex
  justify-content space-between

header nav menu
  display flex
  margin 0 30px 0 0
  color #d2d2d4

header nav label
  color #1e1f24
  margin-right 10px

menu li
  clear right
  margin 0 20px
  text-transform uppercase