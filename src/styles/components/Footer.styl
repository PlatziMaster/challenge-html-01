.Footer
  background-color #1e1f24
  color #ffffff
  display flex
  justify-content space-around
  align-items center
  height 200px
  text-transform uppercase
  &__item
    text-align: center
  &__social
     display: flex
     justify-content: space-between
     text-transform:  none
  &__btn
     background-color #ff565b
     color: #FFFFFF
     padding 0 20px
     border solid 1px #ff565b
  &__form
     background-color #ffffff
     height 35px
  &__subscribe
     height 35px
     display: flex
    
